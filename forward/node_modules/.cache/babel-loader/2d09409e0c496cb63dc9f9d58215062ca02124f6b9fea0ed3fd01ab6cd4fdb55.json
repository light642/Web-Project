{"ast":null,"code":"import { ref } from 'vue';\nimport axios from 'axios';\nexport default {\n  __name: 'UserFile',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const directory = ref({});\n    axios.get('/dir/user').then(res => {\n      directory.value = res.data;\n    });\n    const blocked = () => {\n      alert('删除@click、关闭mock后可用');\n    };\n    const remove = (node, data) => {\n      console.log(node);\n      console.log(data);\n      alert(\"delete\");\n      const parent = node.parent;\n      const children = parent.data.children || parent.data;\n      const index = children.findIndex(d => d.id === data.id);\n      children.splice(index, 1);\n      directory.value = [...directory.value];\n    };\n    const __returned__ = {\n      directory,\n      blocked,\n      remove,\n      ref,\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","axios","directory","get","then","res","value","data","blocked","alert","remove","node","console","log","parent","children","index","findIndex","d","id","splice"],"sources":["C:/Users/Lenovo/Documents/webProj/web/src/views/user/UserFile.vue"],"sourcesContent":["<template>\r\n    <el-card>\r\n        <el-upload\r\n            action=\"/d\"\r\n            multiple \r\n            :show-file-list=\"false\"\r\n            style=\"height: 40px;\"\r\n        >\r\n            <el-button size=\"small\" @click.prevent=\"blocked\">添加文件</el-button>\r\n        </el-upload>\r\n\r\n        <el-tree style=\"max-width: 600px\" :data=\"directory\" show-checkbox node-key=\"id\" :expand-on-click-node=\"false\">\r\n            <template #default=\"{ node, data }\">\r\n                <span class=\"custom-tree-node\">\r\n                    <span>{{ node.label }}</span>\r\n                    <span>\r\n                        <a @click=\"remove(node, data)\" style=\"color: deeppink;\"> Delete </a>\r\n                    </span>\r\n                </span>\r\n            </template>\r\n        </el-tree>\r\n    </el-card>\r\n</template>\r\n<script setup>\r\nimport { ref } from 'vue'\r\nimport axios from 'axios';\r\n\r\nconst directory = ref({})\r\naxios.get('/dir/user').then((res) => {\r\n    directory.value = res.data;\r\n})\r\nconst blocked = ()=>{\r\n    alert('删除@click、关闭mock后可用')\r\n}\r\nconst remove = (node, data) => {\r\n    console.log(node)\r\n    console.log(data)\r\n    alert(\"delete\")\r\n    const parent = node.parent;\r\n    const children = parent.data.children || parent.data;\r\n    const index = children.findIndex((d) => d.id === data.id);\r\n    children.splice(index, 1);\r\n    directory.value = [...directory.value];\r\n}\r\n</script>\r\n<style>\r\n.custom-tree-node {\r\n    flex: 1;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    font-size: 14px;\r\n    padding-right: 8px;\r\n}\r\n</style>"],"mappings":"AAwBA,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,KAAK,MAAM,OAAO;;;;;;;IAEzB,MAAMC,SAAS,GAAGF,GAAG,CAAC,CAAC,CAAC,CAAC;IACzBC,KAAK,CAACE,GAAG,CAAC,WAAW,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;MACjCH,SAAS,CAACI,KAAK,GAAGD,GAAG,CAACE,IAAI;IAC9B,CAAC,CAAC;IACF,MAAMC,OAAO,GAAGA,CAAA,KAAI;MAChBC,KAAK,CAAC,oBAAoB,CAAC;IAC/B,CAAC;IACD,MAAMC,MAAM,GAAGA,CAACC,IAAI,EAAEJ,IAAI,KAAK;MAC3BK,OAAO,CAACC,GAAG,CAACF,IAAI,CAAC;MACjBC,OAAO,CAACC,GAAG,CAACN,IAAI,CAAC;MACjBE,KAAK,CAAC,QAAQ,CAAC;MACf,MAAMK,MAAM,GAAGH,IAAI,CAACG,MAAM;MAC1B,MAAMC,QAAQ,GAAGD,MAAM,CAACP,IAAI,CAACQ,QAAQ,IAAID,MAAM,CAACP,IAAI;MACpD,MAAMS,KAAK,GAAGD,QAAQ,CAACE,SAAS,CAAEC,CAAC,IAAKA,CAAC,CAACC,EAAE,KAAKZ,IAAI,CAACY,EAAE,CAAC;MACzDJ,QAAQ,CAACK,MAAM,CAACJ,KAAK,EAAE,CAAC,CAAC;MACzBd,SAAS,CAACI,KAAK,GAAG,CAAC,GAAGJ,SAAS,CAACI,KAAK,CAAC;IAC1C,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}