{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      pageTitle: '会议室信息',\n      meetingRooms: [],\n      addDialogVisible: false,\n      addDialogLoading: false,\n      newMeetingRoom: {\n        name: '',\n        capacity: null\n      },\n      addFormRules: {\n        name: [{\n          required: true,\n          message: '请输入会议室名称',\n          trigger: 'blur'\n        }],\n        capacity: [{\n          required: true,\n          message: '请输入容纳人数',\n          trigger: 'blur'\n        }, {\n          type: 'number',\n          message: '请输入有效的数字',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    if (localStorage.getItem(\"userId\") == null) {\n      this.$router.push(\"/login\");\n    }\n    this.getMeetingRoomList();\n  },\n  mounted() {},\n  methods: {\n    getOccupiedTime(hoursStatus) {\n      const occupiedRanges = this.calculateTimeRanges(hoursStatus, '1');\n      return this.formatTimeRanges(occupiedRanges);\n    },\n    // 计算空闲时间\n    getAvailableTime(hoursStatus) {\n      const availableRanges = this.calculateTimeRanges(hoursStatus, '0');\n      return this.formatTimeRanges(availableRanges);\n    },\n    // 计算时间段\n    calculateTimeRanges(hoursStatus, targetStatus) {\n      const ranges = [];\n      let start = null;\n      for (let i = 0; i < hoursStatus.length; i++) {\n        if (hoursStatus[i] === targetStatus) {\n          if (start === null) {\n            start = i;\n          }\n        } else {\n          if (start !== null) {\n            ranges.push([start, i - 1]);\n            start = null;\n          }\n        }\n      }\n      if (start !== null) {\n        ranges.push([start, hoursStatus.length - 1]);\n      }\n      return ranges;\n    },\n    // 格式化时间段\n    formatTimeRanges(ranges) {\n      return ranges.map(range => {\n        const startHour = range[0];\n        const endHour = range[1];\n        return `${startHour}:00 - ${endHour + 1}:00`;\n      }).join(', ');\n    },\n    getMeetingRoomList() {\n      this.$http.get(\"/meetingRoom/getAll\").then(res => {\n        console.log(res);\n        this.meetingRooms = res.data.body;\n      });\n    },\n    showAddDialog() {\n      this.addDialogVisible = true;\n    },\n    resetAddDialog() {\n      this.addDialogVisible = false;\n      this.$refs.addForm.resetFields();\n    },\n    addMeetingRoom() {\n      this.$refs.addForm.validate(valid => {\n        if (valid) {\n          this.addDialogLoading = true;\n          // 模拟异步请求\n          this.$http.post(\"/meetingRoom/createMeetingRoom/\", this.newMeetingRoom).then(res => {\n            console.log(res.data.code);\n            console.log(res.data.code == 200);\n            if (res.data.code === 200) {\n              this.$message.success(\"添加会议室成功\");\n              this.getMeetingRoomList();\n            }\n          }).catch(err => {\n            console.log(err);\n            this.$message.error(\"内部异常，添加失败\");\n          });\n          this.resetAddDialog();\n          this.addDialogLoading = false;\n          this.getMeetingRoomList();\n        }\n      });\n    },\n    deleteMeetingRoom(id) {\n      console.log(JSON.stringify(id));\n      this.$http.post(`/meetingRoom/deleteMeetingRoomById/${id}`).then(res => {\n        console.log(res);\n        if (res.data.code == 200) {\n          this.$message.success(\"删除成功\");\n          this.getMeetingRoomList();\n        } else {\n          this.$message.error(\"内部异常，删除失败\");\n        }\n      }).catch(err => {\n        this.$message.error(\"内部异常，删除失败\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","pageTitle","meetingRooms","addDialogVisible","addDialogLoading","newMeetingRoom","name","capacity","addFormRules","required","message","trigger","type","created","localStorage","getItem","$router","push","getMeetingRoomList","mounted","methods","getOccupiedTime","hoursStatus","occupiedRanges","calculateTimeRanges","formatTimeRanges","getAvailableTime","availableRanges","targetStatus","ranges","start","i","length","map","range","startHour","endHour","join","$http","get","then","res","console","log","body","showAddDialog","resetAddDialog","$refs","addForm","resetFields","addMeetingRoom","validate","valid","post","code","$message","success","catch","err","error","deleteMeetingRoom","id","JSON","stringify"],"sources":["C:\\Users\\Lenovo\\Documents\\webProj\\web\\src\\views\\admin\\RoomView.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <h2>{{ pageTitle }}</h2>\r\n        <div class=\"table-header\">\r\n            <el-button type=\"primary\" @click=\"showAddDialog\">新建会议室</el-button>\r\n        </div>\r\n        <el-table :data=\"meetingRooms\" style=\"width: 100%\">\r\n            <el-table-column prop=\"name\" label=\"会议室名称\" />\r\n            <el-table-column prop=\"capacity\" label=\"容纳人数\" />\r\n            <el-table-column prop=\"status\" label=\"状态\" />\r\n            <el-table-column prop=\"theme\" label=\"主题\" />\r\n            <el-table-column label=\"已占用时间\">\r\n                <template v-if=\"true\" v-slot:default=\"scope\">\r\n                    {{ this.getOccupiedTime(scope.row.hoursStatus) }}\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"空闲时间\">\r\n                <template v-if=\"true\" v-slot:default=\"scope\">\r\n                    {{ this.getAvailableTime(scope.row.hoursStatus) }}\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\">\r\n                <template v-if=\"true\" v-slot:default=\"scope\">\r\n                    <el-button type=\"danger\" @click=\"deleteMeetingRoom(scope.row.id)\">删除</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <el-dialog title=\"新建会议室\" v-model=\"addDialogVisible\" @close=\"resetAddDialog\">\r\n            <el-form :model=\"newMeetingRoom\" ref=\"addForm\" :rules=\"addFormRules\" label-width=\"80px\">\r\n                <el-form-item label=\"会议室名称\" prop=\"name\">\r\n                    <el-input v-model=\"newMeetingRoom.name\" />\r\n                </el-form-item>\r\n                <el-form-item label=\"容纳人数\" prop=\"capacity\">\r\n                    <el-input-number v-model=\"newMeetingRoom.capacity\" :min=\"1\" />\r\n                </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"resetAddDialog\">取消</el-button>\r\n                <el-button type=\"primary\" :loading=\"addDialogLoading\" @click=\"addMeetingRoom\">确定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            pageTitle: '会议室信息',\r\n            meetingRooms: [\r\n            ],\r\n            addDialogVisible: false,\r\n            addDialogLoading: false,\r\n            newMeetingRoom: {\r\n                name: '',\r\n                capacity: null\r\n            },\r\n            addFormRules: {\r\n                name: [\r\n                    { required: true, message: '请输入会议室名称', trigger: 'blur' }\r\n                ],\r\n                capacity: [\r\n                    { required: true, message: '请输入容纳人数', trigger: 'blur' },\r\n                    { type: 'number', message: '请输入有效的数字', trigger: 'blur' }\r\n                ]\r\n            }\r\n        };\r\n    },\r\n    created() {\r\n\r\n        if (localStorage.getItem(\"userId\") == null) {\r\n            this.$router.push(\"/login\")\r\n        }\r\n        this.getMeetingRoomList()\r\n    },\r\n    mounted() { },\r\n    methods: {\r\n        getOccupiedTime(hoursStatus) {\r\n            const occupiedRanges = this.calculateTimeRanges(hoursStatus, '1');\r\n            return this.formatTimeRanges(occupiedRanges);\r\n        },\r\n        // 计算空闲时间\r\n        getAvailableTime(hoursStatus) {\r\n            const availableRanges = this.calculateTimeRanges(hoursStatus, '0');\r\n            return this.formatTimeRanges(availableRanges);\r\n        },\r\n        // 计算时间段\r\n        calculateTimeRanges(hoursStatus, targetStatus) {\r\n            const ranges = [];\r\n            let start = null;\r\n\r\n            for (let i = 0; i < hoursStatus.length; i++) {\r\n                if (hoursStatus[i] === targetStatus) {\r\n                    if (start === null) {\r\n                        start = i;\r\n                    }\r\n                } else {\r\n                    if (start !== null) {\r\n                        ranges.push([start, i - 1]);\r\n                        start = null;\r\n                    }\r\n                }\r\n            }\r\n\r\n            if (start !== null) {\r\n                ranges.push([start, hoursStatus.length - 1]);\r\n            }\r\n\r\n            return ranges;\r\n        },\r\n        // 格式化时间段\r\n        formatTimeRanges(ranges) {\r\n            return ranges\r\n                .map(range => {\r\n                    const startHour = range[0];\r\n                    const endHour = range[1];\r\n                    return `${startHour}:00 - ${endHour + 1}:00`;\r\n                })\r\n                .join(', ');\r\n        },\r\n        getMeetingRoomList() {\r\n            this.$http.get(\"/meetingRoom/getAll\").then(res => {\r\n                console.log(res)\r\n                this.meetingRooms = res.data.body\r\n            })\r\n\r\n        },\r\n        showAddDialog() {\r\n            this.addDialogVisible = true;\r\n        },\r\n        resetAddDialog() {\r\n            this.addDialogVisible = false;\r\n            this.$refs.addForm.resetFields();\r\n        },\r\n\r\n        addMeetingRoom() {\r\n            this.$refs.addForm.validate((valid) => {\r\n                if (valid) {\r\n                    this.addDialogLoading = true;\r\n                    // 模拟异步请求\r\n                    this.$http.post(\"/meetingRoom/createMeetingRoom/\", this.newMeetingRoom).then((res) => {\r\n                        console.log(res.data.code)\r\n                        console.log(res.data.code == 200)\r\n\r\n                        if (res.data.code === 200) {\r\n                            this.$message.success(\"添加会议室成功\")\r\n                            this.getMeetingRoomList()\r\n                        }\r\n                    }).catch((err) => {\r\n                        console.log(err)\r\n                        this.$message.error(\"内部异常，添加失败\")\r\n                    });\r\n                    this.resetAddDialog();\r\n                    this.addDialogLoading = false;\r\n                    this.getMeetingRoomList()\r\n                }\r\n            });\r\n        },\r\n        deleteMeetingRoom(id) {\r\n            console.log(JSON.stringify(id));\r\n            this.$http.post(`/meetingRoom/deleteMeetingRoomById/${id}`).then((res) => {\r\n                console.log(res)\r\n                if (res.data.code == 200) {\r\n                    this.$message.success(\"删除成功\")\r\n                    this.getMeetingRoomList()\r\n                } else {\r\n                    this.$message.error(\"内部异常，删除失败\")\r\n                }\r\n            }).catch((err) => {\r\n                this.$message.error(\"内部异常，删除失败\")\r\n            });\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nh2 {\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.table-header {\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.hours-status-cell {\r\n    display: flex;\r\n}\r\n\r\n.hour-cell {\r\n    flex: 1;\r\n    height: 20px;\r\n    border: 1px solid #ccc;\r\n}\r\n\r\n.occupied {\r\n    background-color: red;\r\n}\r\n\r\n.available {\r\n    background-color: green;\r\n}\r\n</style>"],"mappings":";AA6CA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,OAAO;MAClBC,YAAY,EAAE,EACb;MACDC,gBAAgB,EAAE,KAAK;MACvBC,gBAAgB,EAAE,KAAK;MACvBC,cAAc,EAAE;QACZC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE;MACd